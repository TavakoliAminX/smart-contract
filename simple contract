pragma solidity 0.8.20;
contract simple {
    uint256 public x;
    uint256 public y;
    address public owner;
    uint256 public createAt;

    constructor(uint256 _y, uint256 _x) {
        y = _x;
        x = _y;
        owner = msg.sender;
        createAt = block.timestamp;
    }
}
